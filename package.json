{
  "name": "@n-rowe/pash",
  "type": "module",
  "version": "0.2.1",
  "packageManager": "pnpm@10.4.1",
  "description": "A fast, minimal json patch generation library based off ohash's implementation",
  "author": {
    "name": "Nathan Rowe"
  },
  "license": "MIT",
  "repository": {
    "url": "https://github.com/n-rowe/pash"
  },
  "sideEffects": false,
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "bench": "NODE_OPTIONS=--max-old-space-size=8192 vitest bench",
    "bench:ci": "NODE_OPTIONS=--max-old-space-size=8192 vitest bench --run",
    "prepack": "unbuild",
    "prepare": "husky",
    "lint": "eslint --cache .",
    "lint:fix": "eslint --cache --fix .",
    "test": "vitest",
    "test:ci": "vitest run --coverage"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^4.3.0",
    "@codspeed/vitest-plugin": "^4.0.0",
    "@faker-js/faker": "^9.5.0",
    "@types/node": "^22.13.5",
    "@vitest/coverage-v8": "^3.0.7",
    "eslint": "^9.21.0",
    "fast-json-patch": "^3.1.1",
    "husky": "^9.1.7",
    "ohash": "^2.0.4",
    "std-env": "^3.8.0",
    "typescript": "^5.7.3",
    "unbuild": "^3.3.1",
    "vitest": "^3.0.7"
  },
  "volta": {
    "node": "22.14.0",
    "pnpm": "10.4.1"
  },
  "lint-staged": {
    "*.{ts,js,mjs}": [
      "eslint --cache --fix",
      "vitest related --run"
    ]
  }
}
